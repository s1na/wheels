{
  "name": "wheels",
  "version": "0.0.1",
  "description": "Car sharing dapp",
  "main": "app/index.js",
  "private": true,
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "jest",
    "build": "webpack",
    "serve": "webpack-serve --config webpack.config.js",
    "lint": "standard",
    "flow": "flow",
    "contracts:build": "truffle compile",
    "contracts:test": "truffle test",
    "contracts:lint": "solium -d contracts",
    "contracts:deploy": "truffle migrate --reset --compile-all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/s1na/wheels.git"
  },
  "contributors": [
    "Omid Najaee",
    "Sina Mahmoodi"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/s1na/wheels/issues"
  },
  "homepage": "https://github.com/s1na/wheels#readme",
  "standard": {
    "envs": [
      "mocha"
    ],
    "globals": [
      "assert",
      "contract",
      "artifacts"
    ]
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "ipfs-api": "^22.1.1",
    "openzeppelin-solidity": "^1.10.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-hot-loader": "^4.3.3",
    "react-router-dom": "^4.3.1",
    "truffle-contract": "^3.0.6",
    "web3": "^1.0.0-beta.34"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "flow-bin": "^0.75.0",
    "html-webpack-plugin": "^4.0.0-alpha",
    "solium": "^1.1.7",
    "standard": "^11.0.1",
    "truffle": "^4.1.12",
    "webpack": "^4.12.1",
    "webpack-cli": "^3.0.8",
    "webpack-serve": "^1.0.4"
  }
}
